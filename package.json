{
  "name": "vue-onsenui-weather",
  "version": "1.0.0",
  "description": "Vue OnsenUI Weather App",
  "author": "Manuel Saelices <msaelices@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 8080",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:watch": "cross-env NODE_ENV=production WEBPACK_WATCH=true webpack --progress --hide-modules",
    "demo:index": "mv www/index.html www/main.html && sed -i 's/index\\.html/main\\.html/' www/demo.html && mv www/demo.html www/index.html",
    "deploy": "npm run demo:index && ghpages -p www"
  },
  "dependencies": {
    "onsenui": "~2.8.2",
    "vue": "^2.5.8",
    "vue-onsenui": "~2.4.2",
    "vuex": "^3.0.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msaelices/vue-onsenui-weather/issues"
  },
  "homepage": "https://github.com/argelius/react-onsenui-redux-weather#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "postcss": "^6.0.11",
    "postcss-base64": "^0.7.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "postcss-url": "^7.1.2",
    "style-loader": "^0.18.2",
    "vue-loader": "^12.0.4",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.7.1"
  }
}
